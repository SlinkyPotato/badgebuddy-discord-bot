{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "jsc": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    },
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "useDefineForClassFields": true,
      "legacyDecorator": true,
      "decoratorMetadata": true
    }
  },
  "minify": false
}
