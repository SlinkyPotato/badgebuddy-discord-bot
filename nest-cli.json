{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/discord-bot/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": "tsc",
    "typeCheck": true,
    "webpack": true,
    "tsConfigPath": "apps/discord-bot/tsconfig.app.json"
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "discord-bot": {
      "type": "application",
      "root": "apps/discord-bot",
      "entryFile": "main",
      "sourceRoot": "apps/discord-bot/src",
      "compilerOptions": {
        "tsConfigPath": "apps/discord-bot/tsconfig.app.json"
      }
    },
    "processor": {
      "type": "application",
      "root": "apps/processor",
      "entryFile": "main",
      "sourceRoot": "apps/processor/src",
      "compilerOptions": {
        "tsConfigPath": "apps/processor/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/discord-bot"
}